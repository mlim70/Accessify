{
  "manifest_version": 3,
  "name": "All in one accessibility",
  "version": "1.0",
  "description": "Changes the font of web pages to your preferred Google Web Font",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "scripting",
    "tabs",
    "tts"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "http://localhost:3000/*"
      ],
      "js": [
        "content.js",
        "features/font-changer.js",
        "utils/translator.js"
      ],
      "run_at": "document_start"
    }
  ],
"content_security_policy": {
  "extension_pages": "script-src 'self'; object-src 'self'; media-src 'self' blob:;"
},
"background": {
  "service_worker": "background.js"
},
"action": {
  "default_popup": "ui/popup.html",
  "default_icon": "icon.png"
},
