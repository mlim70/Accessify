{
    "manifest_version": 3,
    "name": "Background Color Changer",
    "version": "1.0",
    "description": "Changes the background color of web pages",
    "icons": {
      "48": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  